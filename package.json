{
  "name": "adobexd-to-front-end",
  "version": "1.0.0",
  "description": "adobe xd to front-end code",
  "main": "index.js",
  "repository": "git@github.com:AliTaee/AdobeXD-to-HTML-CSS.git",
  "author": "alitaee <ali.taee.72@gmail.com>",
  "license": "MIT",
  "keywords": [
    "HTML",
    "CSS",
    "SCSS",
    "BEM"
  ],
  "scripts": {
    "dev": "parcel src/index.html --port 9000 --open",
    "build": "parcel build src/index.html",
    "predeploy": "rm -rf dist && parcel build src/index.html --no-source-maps --public-url ./",
    "deploy": "gh-pages -d dist"
  },
  "devDependencies": {
    "gh-pages": "^3.2.3",
    "parcel-bundler": "^1.12.5",
    "parcel-plugin-asset-copier": "^1.1.0",
    "sass": "^1.37.5"
  },
  "assetsPath": "src/assets"
}
